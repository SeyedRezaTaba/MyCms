@model IEnumerable<DataLayer.Page>
<ul>
    @foreach (var item in Model)
    {
        <li>
            <img class="border-radius" src="~/PageImages/@item.ImageName">
            <h2> <a href="">@item.Title</a></h2>
            <span>انتشار : @item.CreaetDate.ToShamsi()</span>
        </li>
    }    
</ul>
